:recordOrBest rdf:type owl:ObjectProperty ;
              rdfs:range skos:Concept ;
              rdfs:isDefinedBy <http://purl.org/athletics#> ;
              rdfs:label "record or best"@en ;
              skos:definition "A record or best is the best result that has ever been achieved in a particular sport or activity, for example the fastest time, the furthest distance, or the greatest number of victories."@en .


###  http://purl.org/athletics#results
:results rdf:type owl:ObjectProperty ;
         rdfs:range rdf:Seq ;
         rdfs:isDefinedBy <http://purl.org/athletics#> ;
         rdfs:label "results"@en ;
         skos:definition "Results of a competition. The list of results —competitors and their performances."@en .


